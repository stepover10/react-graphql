type User {
  id: ID! @id @unique
  name: String!
  lastName: String!
}

type Post {
  files: [Files!]!
  location: String!
  caption: String!
  likes: [Like!]!
  comment: [Comments!]!
}

type Like {
  user: User!
  post: Post!
}

type Commnet {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}