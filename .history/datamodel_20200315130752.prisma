type User {
  id: ID! @id @unique
  unsername: String! @unique
  firstName: String!
  lastName: String!
  bio: String
  posts: [Post!]!
  likes: [Like!]!
  comments: [Comments!]!
}

type Post {
  files: [Files!]!
  location: String
  caption: String!
  likes: [Like!]!
  comment: [Comments!]!
}

type Like {
  user: User!
  post: Post!
}

type Commnet {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}