type User {
  id: ID! @id @unique
  unsername: String! @unique
  email: String! @unique
  emailTest1: String! @unique
  firstName: String @default(value: "")
  lastName: String!
  bio: String
  posts: [Post!]!
  likes: [Like!]!
  comments: [Commnet!]!
  followers: [User!]! @relation(name: "FollowRelation")
  following: [User!]! @relation(name: "FollowRelation")
}

type Post {
  id: ID! @id @unique
  files: [File!]!
  location: String
  caption: String!
  likes: [Like!]!
  comment: [Commnet!]!
}

type Like {
  id: ID! @id @unique
  user: User!
  post: Post!
}

type Commnet {
  id: ID! @id @unique
  text: String!
  user: User!
  post: Post!
}

type File {
  id: ID! @id @unique
  url: String!
  post: Post!
}